    "def loss_func(F, A):\n",
    "    if len(F) != len(A):\n",
    "        raise BaseException\n",
    "    res = 0\n",
    "    for i in range(len(F)):\n",
    "        res += float(abs(F[i] - A[i]) / (abs(A[i]) + abs(F[i])))\n",
    "    res *= float(200) / len(F)\n",
    "    return res"
   ]
